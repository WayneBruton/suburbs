 <% include partials/header %>



    <div class="container col-sm-12" id="mainform" style="width: 100vw; display: flex; justify-content: center; margin-top: 55px">
            
            <div id="signupbox" style=" margin-top:50px" class="mainbox col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
    
                <div class="panel panel-info">
                    <div class="panel-body">
                        <form class="form-horizontal" role="form" method="post" action="/signup" onSubmit="return checkblank(this);">
                            <h1>Register</h1>
                            <div class="form-group">
                                <label for="first_name" class="col-md-3 control-label">Business Name</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" name="first_name" placeholder="Name of Business">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="last_name" class="col-md-3 control-label">Owner Name</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" name="last_name" placeholder="Owner Name">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="mob_no" class="col-md-3 control-label">Mobile Number</label>
                                <div class="col-md-9">
                                    <input type="number" class="form-control" name="mob_no" placeholder="Mobile Number">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="email" class="col-md-3 control-label">Website</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" name="email" placeholder="Website">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="email" class="col-md-3 control-label">Email Address</label>
                                <div class="col-md-9">
                                    <input type="email" class="form-control" name="email" placeholder="email address">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="email" class="col-md-3 control-label">Package Option</label>
                                <div class="col-md-9">
                                    <input type="tyext" class="form-control" name="email" placeholder="Package option">
                                </div>
                            </div>
                            <div class="form-group">
                                <label for="email" class="col-md-3 control-label">Area</label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" name="email" placeholder="Area">
                                </div>
                            </div>
                            <input class="btn btn-outline-success my-2 my-sm-0" type="submit" value="Submit">
                            
                        </form>
                        <br><br><br>
                    </div>
                </div>
            </div>
        </div>



</body>
</html>